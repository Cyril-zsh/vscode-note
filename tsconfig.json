{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "outDir": "out",
        "lib": ["es7", "dom"],
        "sourceMap": true,
        "jsx": "preserve",
        "strict": true,
        "noUnusedLocals": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": false,
        "noImplicitAny": true
    },
    "exclude": ["node_modules"]
}
