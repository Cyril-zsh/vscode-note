{
    "name": "vscode-note",
    "displayName": "vscode-note",
    "description": "",
    "version": "0.0.1",
    "publisher": "shinhwagk",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension",
    "contributes": {
        "configuration": [
            {
                "title": "",
                "properties": {
                    "vsnote.dbpath": {
                        "type": "string",
                        "default": "~/.vscode-note",
                        "description": ""
                    },
                    "vsnote.updateStrategy": {
                        "type": "string",
                        "enum": [
                            "LATEST",
                            "SEMVER"
                        ],
                        "default": "LATEST",
                        "description": "what strategy you want to use to update your package.json"
                    }
                }
            }
        ],
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && jest"
    },
    "devDependencies": {
        "@types/jest": "^23.3.11",
        "@types/node": "^10.12.18",
        "@types/object-path": "^0.11.0",
        "@types/rimraf": "^2.0.2",
        "@types/untildify": "^3.0.0",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.5",
        "tslint": "^5.12.0",
        "typescript": "^3.2.2",
        "vscode": "^1.1.26"
    },
    "dependencies": {
        "object-path": "^0.11.4",
        "rimraf": "^2.6.3",
        "untildify": "^3.0.3"
    }
}
